{
  "manifest_version": 3,
  "name": "Notiondipity",
  "version": "0.1.0",
  "background": {
    "scripts": [
      "dist/Worker.js"
    ]
  },
  "permissions": [
    "tabs",
    "identity",
    "storage"
  ],
  "action": {
    "default_title": "Notiondipity",
    "default_popup": "dist/index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "notiondipity@svana.name"
    }
  }
}
